
password = input("Enter password: ")

upper = lower = digit = special = 0

if len(password) < 8:
    print("Weak Password")
else:
    for ch in password:
        if ch.isupper():
            upper = 1
        elif ch.islower():
            lower = 1
        elif ch.isdigit():
            digit = 1
        else:
            special = 1

    if upper and lower and digit and special:
        print("Strong Password")
    else:
        print("Moderate Password")

